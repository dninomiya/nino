{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [
        "AI_GATEWAY_API_KEY",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL",
        "CRON_SECRET",
        "DISCORD_BOT_TOKEN",
        "DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET",
        "GITHUB_ACCESS_TOKEN",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "NEXT_PUBLIC_BASE_URL",
        "NEXT_PUBLIC_VERCEL_BRANCH_URL",
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_VERCEL_PROJECT_PRODUCTION_URL",
        "NEXT_PUBLIC_VERCEL_URL",
        "REGISTRY_JWT_SECRET",
        "NODE_ENV",
        "NOTION_TOKEN",
        "PORT",
        "SENTRY_AUTH_TOKEN",
        "STRIPE_COMMUNITY_ANNUAL_DISCOUNT_PRICE_ID",
        "STRIPE_COMMUNITY_PRICE_ID",
        "STRIPE_MENTOR_ANNUAL_DISCOUNT_PRICE_ID",
        "STRIPE_MENTOR_PRICE_ID",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "TURSO_AUTH_TOKEN",
        "TURSO_DATABASE_URL",
        "R2_ACCOUNT_ID",
        "R2_ACCESS_KEY_ID",
        "R2_SECRET_ACCESS_KEY",
        "R2_BUCKET_NAME",
        "R2_PUBLIC_URL"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    }
  }
}
